        <div class="container">
            <div class="row justify-content-center" v-for="(item, index) in dbres">
                <div class="col-lg-10" style="margin: 16px 0px;">
                    <div class="media show-ngo-card  p-8">
                        <div class="media-body media-card-body">
                            <h5 class="mt-0 mb-16"><b>{{item.caseTitle}}</b></h5>
                            <div class="d-flex">
                                <label class="c-label col-sm-2 col-no-p" for="email">Location:</label>
                                <p class="col-sm-10 col-no-p">{{item.location}}</p>
                            </div>
                            <div class="d-flex">
                                <label class="c-label col-sm-2 col-no-p" for="email">Victims:</label>
                                <p class="col-sm-10 col-no-p">
                                    <span v-for="(victim, index) in item.victims" class="badge badge-primary" style="margin-right: 2px">{{victim}}</span>
                                </p>
                            </div>

                            <div class="d-flex">
                                <label class="c-label col-sm-2 col-no-p" for="email">Complaint:</label>
                                <p class="col-sm-10 col-no-p">{{item.complaint}}</p>
                            </div>
                            <p class="heading">Accused</p>

                            <template v-for="(accu, index) in item.accused">
                                    <div class="d-flex">
                                            <label class="c-label col-sm-2 col-no-p" for="email">Name:</label>
                                            <p class="col-sm-10 col-no-p">{{accu.name}}</p>
                                    </div>
                                    <div class="d-flex">
                                            <label class="c-label col-sm-2 col-no-p" for="email">Address:</label>
                                            <p class="col-sm-10 col-no-p">{{accu.description}}</p>
                                    </div>
                            </template>

                            <p class="heading">Witness</p>
                            <template v-for="(wit, index) in item.witness">
                                    <div class="d-flex">
                                            <label class="c-label col-sm-2 col-no-p" for="email">Name:</label>
                                            <p class="col-sm-10 col-no-p">{{wit.name}}</p>
                                    </div>
                                    <div class="d-flex">
                                            <label class="c-label col-sm-2 col-no-p" for="email">Address:</label>
                                            <p class="col-sm-10 col-no-p">{{wit.description}}</p>
                                    </div>
                            </template>


                            <div class="d-flex">
                                <label class="c-label col-sm-2 col-no-p" for="email">Offense Sections:</label>
                                <p class="col-sm-10 col-no-p">
                                    <span v-for="(os, index) in item.offenseSections" class="badge badge-success" style="margin-right: 2px">{{os}}</span>
                                </p>
                            </div>
                            <br>
                            <!-- <a href="#" class="txt1">Read More</a> -->
                        </div>
                    </div>
                </div>

